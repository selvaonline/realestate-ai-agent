# RealEstate AI Agent - Combined gitignore

# Node.js dependencies
node_modules/
**/node_modules/
jspm_packages/

# Build outputs
dist/
build/
out/
**/*.tsbuildinfo

# Angular specific
.angular/
**/.angular/
coverage/

# Playwright / E2E testing
playwright-report/
playwright/.cache/
.playwright/
test-results/

# Environment & secrets
.env
.env.*
**/.env
**/.env.*
!.env.example

# BUT keep Angular environment files (needed for build)
!**/src/environments/environment.ts
!**/src/environments/environment.prod.ts

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*
lerna-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Coverage directory used by tools like istanbul
coverage
*.lcov
.nyc_output

# Optional npm cache directory
.npm
.eslintcache
.stylelintcache

# Optional REPL history
.node_repl_history

# Output of 'npm pack'
*.tgz

# Yarn Integrity file
.yarn-integrity

# Caches
.cache/
.parcel-cache/
.temp

# Next.js build output
.next

# Nuxt.js build / generate output
.nuxt

# Gatsby files
.cache/

# Serverless directories
.serverless/

# OS / IDE
.DS_Store
.vscode/
.idea/

# Diagnostic reports
report.[0-9]*.[0-9]*.[0-9]*.[0-9]*.json

# Yarn v3
.pnp.*
.yarn/*
!.yarn/patches
!.yarn/plugins
!.yarn/releases
!.yarn/sdks
!.yarn/versions
