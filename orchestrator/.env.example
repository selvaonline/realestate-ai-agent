# OpenAI Configuration
OPENAI_API_KEY=sk-proj-...
OPENAI_MODEL=gpt-4o-mini

# Optional: Serper.dev for web search (https://serper.dev)
SERPER_API_KEY=

# Browser settings
BROWSER_HEADED=false
BROWSER_DEVTOOLS=false
BROWSER_ENGINE=chromium
BROWSER_SLOWMO=0

# Bright Data proxy (optional - for bypassing Cloudflare)
BRIGHTDATA_USERNAME=
BRIGHTDATA_PASSWORD=
BRIGHTDATA_HOST=brd.superproxy.io:33335
BRIGHTDATA_SESSION=

# Screenshot settings
CAPTURE_ON_BLOCK=true

# PE Scoring & Analytics
# PE_MODE: "private" (default, family office style) or "institutional" (PGIM-style: heavier tenant/market weights, deal size filters)
PE_MODE=private
SKIP_EXTRACTION=true
MIN_BROWSE_SCORE=70

PORT=3001

# Browser Automation Settings (Optional)
# BROWSER_HEADED=false          # Set to true to see browser window
# BROWSER_DEVTOOLS=false        # Set to true to open DevTools (Chromium only)
